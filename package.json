{
  "main": "app/html/index.html",
  "name": "微信web开发者工具old",
  "version": "0.7.0",
  "window": {
    "title": "微信web开发者工具 v0.7.0",
    "icon": "./app/images/icon.png",
    "width": 799,
    "height": 799,
    "frame": true,
    "toolbar": true
  },
  "inject_js_start": "app/dist/inject/jweixindebug.js",
  "inject_js_end": "app/dist/inject/devtools.js",
  "chromium-args": "-ignore-certificate-errors",
  "webview": {
     "partitions": [
        {
          "name": "trusted",
          "accessible_resources": [ "<all_urls>" ]
        },
        {
          "name": "devtools",
          "accessible_resources": [ "<all_urls>" ]
        }
     ]
  }
}
